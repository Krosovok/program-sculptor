<UserControl x:Class="Services.Dialog.SolutionDetails"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dialogViewModel="clr-namespace:DialogViewModel;assembly=DialogViewModel"
             xmlns:common="clr-namespace:UI.Controls.Common;assembly=UI.Controls.Common"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             d:DataContext="{d:DesignInstance dialogViewModel:OtherSolution}">
    <DockPanel>

        <TextBlock Text="{Binding .Solution.Name}"
                   DockPanel.Dock="Top"
                   Margin="5" />

        <common:TypesView/>

    </DockPanel>
</UserControl>